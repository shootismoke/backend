{
  "name": "@shootismoke/backend",
  "version": 2,
  "alias": ["shootismoke.now.sh"],
  "builds": [{ "src": "/api/**/*.ts", "use": "@now/node" }],
  "env": {
    "MONGODB_ATLAS_URI": "@shootismoke-production-mongodb-atlas-uri",
    "AQICN_TOKEN": "@shootismoke-production-aqicn-token"
  },
  "public": false,
  "routes": [
    {
      "src": "/api/graphql",
      "dest": "/api/graphql.ts"
    },
    {
      "src": "/api/push",
      "dest": "/api/push/index.ts"
    }
  ],
  "scope": "shootismoke"
}
