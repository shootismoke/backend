{
  "name": "backend-production",
  "version": 2,
  "alias": ["shootismoke.now.sh"],
  "builds": [{ "src": "/api/**/*.ts", "use": "@now/node" }],
  "env": {
    "ENGINE_API_KEY": "@shootismoke-production-engine-api-key",
    "HAWK_KEY_1_5_0": "@shootismoke-production-hawk-key-1-5-0",
    "MONGODB_ATLAS_URI": "@shootismoke-production-mongodb-atlas-uri",
    "SENTRY_DSN": "shootismoke-production-sentry-dsn"
  },
  "public": false,
  "routes": [
    {
      "src": "/api/graphql",
      "dest": "/api/graphql.ts"
    }
  ],
  "scope": "shootismoke"
}
