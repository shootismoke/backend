{
  "name": "backend",
  "version": 2,
  "alias": ["staging.shootismoke.now.sh"],
  "builds": [{ "src": "/api/**/*.ts", "use": "@now/node" }],
  "env": {
    "HAWK_KEY_1_5_0": "@shootismoke-staging-hawk-key-1-5-0",
    "MONGODB_ATLAS_URI": "@shootismoke-staging-mongodb-atlas-uri"
  },
  "public": false,
  "routes": [
    {
      "src": "/api/graphql",
      "dest": "/api/graphql.ts"
    }
  ],
  "scope": "shootismoke"
}
