{
  "name": "@shootismoke/backend",
  "version": 2,
  "alias": ["shootismoke.now.sh"],
  "env": {
    "MONGODB_ATLAS_URI": "@shootismoke-mongodb-atlas-uri"
  },
  "routes": [
    {
      "src": "/api/graphql",
      "methods": ["GET", "POST"],
      "dest": "/api/graphql.ts"
    },
    {
      "src": "/api/users",
      "methods": ["POST"],
      "dest": "/api/users/create.ts"
    }
  ]
}
