{
  "version": 2,
  "alias": ["staging.shootismoke.now.sh"],
  "builds": [{ "src": "/src/api/**/*.ts", "use": "@now/node" }],
  "env": {
    "AQICN_TOKEN": "@backend-staging-aqicn-token",
    "ENGINE_API_KEY": "@backend-staging-engine-api-key",
    "HAWK_KEY_1_5_0": "@backend-staging-hawk-key-1-5-0",
    "MONGODB_ATLAS_URI": "@backend-staging-mongodb-atlas-uri",
    "SENTRY_DSN": ""
  },
  "public": false,
  "routes": [
    {
      "src": "/api/graphql",
      "dest": "/src/api/graphql.ts"
    },
    {
      "src": "/api/push",
      "dest": "/src/api/push.ts"
    },
    {
      "src": "/api/receipts",
      "dest": "/src/api/receipts.ts"
    }
  ],
  "scope": "shootismoke"
}
