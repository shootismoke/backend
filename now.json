{
  "name": "@shootismoke/backend",
  "version": 2,
  "alias": ["staging.shootismoke.now.sh"],
  "builds": [{ "src": "/packages/microservices/**/*.ts", "use": "@now/node" }],
  "env": {
    "MONGODB_ATLAS_URI": "@shootismoke-staging-mongodb-atlas-uri",
    "WAQI_TOKEN": "@shootismoke-staging-waqi-token"
  },
  "public": false,
  "routes": [
    {
      "src": "/api/graphql",
      "dest": "/packages/microservices/api/graphql.ts"
    }
  ],
  "scope": "shootismoke"
}
