{
  "version": 2,
  "builds": [{ "src": "/src/api/**/*.ts", "use": "@vercel/node" }],
  "public": false,
  "routes": [
    {
      "src": "/api/graphql",
      "dest": "/src/api/graphql.ts"
    },
    {
      "src": "/api/push",
      "dest": "/src/api/push.ts"
    },
    {
      "src": "/api/receipts",
      "dest": "/src/api/receipts.ts"
    }
  ],
  "scope": "shootismoke"
}
